@model EntityLayer.Concrete.Logs

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}


<br/>
<div class="card card-primary">
  <div class="card-header">
    <h3 class="card-title">Log Detayı</h3>
  </div>
  <!-- /.card-header -->
  <!-- form start --> 
    <div class="card-body">
      <div class="form-group">
        <label for="exampleInputEmail1">Log Id</label>
        <input type="text" id="country" name="country" value="@Model.Id" readonly>
      </div>
      <div class="form-group">
        <label for="exampleInputEmail1">Log Mesajı</label>
        <textarea id="w3review" name="w3review" rows="4" cols="50">@Model.Message</textarea>
      </div>
      <div class="form-group">
        <label for="exampleInputEmail1">Log Leveli</label>
        <input type="text" id="country" name="country" value="@Model.Level" readonly>
      </div>
      <div class="form-group">
        <label for="exampleInputEmail1">Log Tarihi</label>
        <input type="text" id="country" name="country" value="@Model.TimeStamp" readonly>
      </div>
      <div class="form-group">
              <label for="exampleInputEmail1">Log Hata Mesajı</label>
              <textarea id="w3review" name="w3review" rows="4" cols="50">@Model.Exception</textarea>
            </div>
       <div class="form-group">
              <label for="exampleInputEmail1">Log Özellikleri</label>
              <textarea id="w3review" name="w3review" rows="4" cols="50">@Model.Properties</textarea>
            </div>

    </div>
    
    <!-- /.card-body -->

    <div class="card-footer">
      <a href="/Admin/LogList" class="button">Log listesine Geri Dön</a>
    </div>
</div>
